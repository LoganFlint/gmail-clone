#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/19784/Desktop/gmail-clone/node_modules/.pnpm/acorn@7.4.1/node_modules/acorn/bin/node_modules:/mnt/c/Users/19784/Desktop/gmail-clone/node_modules/.pnpm/acorn@7.4.1/node_modules/acorn/node_modules:/mnt/c/Users/19784/Desktop/gmail-clone/node_modules/.pnpm/acorn@7.4.1/node_modules:/mnt/c/Users/19784/Desktop/gmail-clone/node_modules/.pnpm/node_modules:/mnt/c/Users/19784/Desktop/gmail-clone/node_modules:/mnt/c/Users/19784/Desktop/node_modules:/mnt/c/Users/19784/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/19784/Desktop/gmail-clone/node_modules/.pnpm/espree@6.2.1/node_modules/espree/node_modules:/mnt/c/Users/19784/Desktop/gmail-clone/node_modules/.pnpm/espree@6.2.1/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/19784/Desktop/gmail-clone/node_modules/.pnpm/acorn@7.4.1/node_modules/acorn/bin/node_modules:/mnt/c/Users/19784/Desktop/gmail-clone/node_modules/.pnpm/acorn@7.4.1/node_modules/acorn/node_modules:/mnt/c/Users/19784/Desktop/gmail-clone/node_modules/.pnpm/acorn@7.4.1/node_modules:/mnt/c/Users/19784/Desktop/gmail-clone/node_modules/.pnpm/node_modules:/mnt/c/Users/19784/Desktop/gmail-clone/node_modules:/mnt/c/Users/19784/Desktop/node_modules:/mnt/c/Users/19784/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/19784/Desktop/gmail-clone/node_modules/.pnpm/espree@6.2.1/node_modules/espree/node_modules:/mnt/c/Users/19784/Desktop/gmail-clone/node_modules/.pnpm/espree@6.2.1/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../acorn@7.4.1/node_modules/acorn/bin/acorn" "$@"
else
  exec node  "$basedir/../../../../../acorn@7.4.1/node_modules/acorn/bin/acorn" "$@"
fi
