#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/loganflint/Desktop/gmail-clone/node_modules/.pnpm/acorn@8.5.0/node_modules/acorn/bin/node_modules:/Users/loganflint/Desktop/gmail-clone/node_modules/.pnpm/acorn@8.5.0/node_modules/acorn/node_modules:/Users/loganflint/Desktop/gmail-clone/node_modules/.pnpm/acorn@8.5.0/node_modules:/Users/loganflint/Desktop/gmail-clone/node_modules/.pnpm/node_modules:/Users/loganflint/Desktop/gmail-clone/node_modules:/Users/loganflint/Desktop/node_modules:/Users/loganflint/node_modules:/Users/node_modules:/node_modules:/Users/loganflint/Desktop/gmail-clone/node_modules/.pnpm/acorn-jsx@5.3.2_acorn@8.5.0/node_modules/acorn-jsx/node_modules:/Users/loganflint/Desktop/gmail-clone/node_modules/.pnpm/acorn-jsx@5.3.2_acorn@8.5.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/loganflint/Desktop/gmail-clone/node_modules/.pnpm/acorn@8.5.0/node_modules/acorn/bin/node_modules:/Users/loganflint/Desktop/gmail-clone/node_modules/.pnpm/acorn@8.5.0/node_modules/acorn/node_modules:/Users/loganflint/Desktop/gmail-clone/node_modules/.pnpm/acorn@8.5.0/node_modules:/Users/loganflint/Desktop/gmail-clone/node_modules/.pnpm/node_modules:/Users/loganflint/Desktop/gmail-clone/node_modules:/Users/loganflint/Desktop/node_modules:/Users/loganflint/node_modules:/Users/node_modules:/node_modules:/Users/loganflint/Desktop/gmail-clone/node_modules/.pnpm/acorn-jsx@5.3.2_acorn@8.5.0/node_modules/acorn-jsx/node_modules:/Users/loganflint/Desktop/gmail-clone/node_modules/.pnpm/acorn-jsx@5.3.2_acorn@8.5.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../acorn@8.5.0/node_modules/acorn/bin/acorn" "$@"
else
  exec node  "$basedir/../../../../../acorn@8.5.0/node_modules/acorn/bin/acorn" "$@"
fi
