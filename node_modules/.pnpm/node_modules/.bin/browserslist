#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/19784/Desktop/gmail-clone/node_modules/.pnpm/browserslist@4.17.5/node_modules/browserslist/node_modules:/mnt/c/Users/19784/Desktop/gmail-clone/node_modules/.pnpm/browserslist@4.17.5/node_modules:/mnt/c/Users/19784/Desktop/gmail-clone/node_modules/.pnpm/node_modules:/mnt/c/Users/19784/Desktop/gmail-clone/node_modules:/mnt/c/Users/19784/Desktop/node_modules:/mnt/c/Users/19784/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/19784/Desktop/gmail-clone/node_modules/.pnpm/node_modules/browserslist/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/19784/Desktop/gmail-clone/node_modules/.pnpm/browserslist@4.17.5/node_modules/browserslist/node_modules:/mnt/c/Users/19784/Desktop/gmail-clone/node_modules/.pnpm/browserslist@4.17.5/node_modules:/mnt/c/Users/19784/Desktop/gmail-clone/node_modules/.pnpm/node_modules:/mnt/c/Users/19784/Desktop/gmail-clone/node_modules:/mnt/c/Users/19784/Desktop/node_modules:/mnt/c/Users/19784/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/19784/Desktop/gmail-clone/node_modules/.pnpm/node_modules/browserslist/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../browserslist/cli.js" "$@"
else
  exec node  "$basedir/../browserslist/cli.js" "$@"
fi
