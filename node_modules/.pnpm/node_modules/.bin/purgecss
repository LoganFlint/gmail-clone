#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/smith/Desktop/Projects/Github/LaunchMail2/launch-mail/node_modules/.pnpm/purgecss@4.0.3/node_modules/purgecss/bin/node_modules:/Users/smith/Desktop/Projects/Github/LaunchMail2/launch-mail/node_modules/.pnpm/purgecss@4.0.3/node_modules/purgecss/node_modules:/Users/smith/Desktop/Projects/Github/LaunchMail2/launch-mail/node_modules/.pnpm/purgecss@4.0.3/node_modules:/Users/smith/Desktop/Projects/Github/LaunchMail2/launch-mail/node_modules/.pnpm/node_modules:/Users/smith/Desktop/Projects/Github/LaunchMail2/launch-mail/node_modules:/Users/smith/Desktop/Projects/Github/LaunchMail2/node_modules:/Users/smith/Desktop/Projects/Github/node_modules:/Users/smith/Desktop/Projects/node_modules:/Users/smith/Desktop/node_modules:/Users/smith/node_modules:/Users/node_modules:/node_modules:/Users/smith/Desktop/Projects/Github/LaunchMail2/launch-mail/node_modules/.pnpm/node_modules/purgecss/bin/node_modules:/Users/smith/Desktop/Projects/Github/LaunchMail2/launch-mail/node_modules/.pnpm/node_modules/purgecss/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/smith/Desktop/Projects/Github/LaunchMail2/launch-mail/node_modules/.pnpm/purgecss@4.0.3/node_modules/purgecss/bin/node_modules:/Users/smith/Desktop/Projects/Github/LaunchMail2/launch-mail/node_modules/.pnpm/purgecss@4.0.3/node_modules/purgecss/node_modules:/Users/smith/Desktop/Projects/Github/LaunchMail2/launch-mail/node_modules/.pnpm/purgecss@4.0.3/node_modules:/Users/smith/Desktop/Projects/Github/LaunchMail2/launch-mail/node_modules/.pnpm/node_modules:/Users/smith/Desktop/Projects/Github/LaunchMail2/launch-mail/node_modules:/Users/smith/Desktop/Projects/Github/LaunchMail2/node_modules:/Users/smith/Desktop/Projects/Github/node_modules:/Users/smith/Desktop/Projects/node_modules:/Users/smith/Desktop/node_modules:/Users/smith/node_modules:/Users/node_modules:/node_modules:/Users/smith/Desktop/Projects/Github/LaunchMail2/launch-mail/node_modules/.pnpm/node_modules/purgecss/bin/node_modules:/Users/smith/Desktop/Projects/Github/LaunchMail2/launch-mail/node_modules/.pnpm/node_modules/purgecss/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../purgecss/bin/purgecss.js" "$@"
else
  exec node  "$basedir/../purgecss/bin/purgecss.js" "$@"
fi
