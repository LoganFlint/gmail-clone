#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/19784/Desktop/gmail-clone/node_modules/.pnpm/tailwindcss@2.2.17_fa866e6aa6eb85ed70431566fb699b34/node_modules/tailwindcss/lib/node_modules:/mnt/c/Users/19784/Desktop/gmail-clone/node_modules/.pnpm/tailwindcss@2.2.17_fa866e6aa6eb85ed70431566fb699b34/node_modules/tailwindcss/node_modules:/mnt/c/Users/19784/Desktop/gmail-clone/node_modules/.pnpm/tailwindcss@2.2.17_fa866e6aa6eb85ed70431566fb699b34/node_modules:/mnt/c/Users/19784/Desktop/gmail-clone/node_modules/.pnpm/node_modules:/mnt/c/Users/19784/Desktop/gmail-clone/node_modules:/mnt/c/Users/19784/Desktop/node_modules:/mnt/c/Users/19784/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/19784/Desktop/gmail-clone/node_modules/tailwindcss/lib/node_modules:/mnt/c/Users/19784/Desktop/gmail-clone/node_modules/tailwindcss/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/19784/Desktop/gmail-clone/node_modules/.pnpm/tailwindcss@2.2.17_fa866e6aa6eb85ed70431566fb699b34/node_modules/tailwindcss/lib/node_modules:/mnt/c/Users/19784/Desktop/gmail-clone/node_modules/.pnpm/tailwindcss@2.2.17_fa866e6aa6eb85ed70431566fb699b34/node_modules/tailwindcss/node_modules:/mnt/c/Users/19784/Desktop/gmail-clone/node_modules/.pnpm/tailwindcss@2.2.17_fa866e6aa6eb85ed70431566fb699b34/node_modules:/mnt/c/Users/19784/Desktop/gmail-clone/node_modules/.pnpm/node_modules:/mnt/c/Users/19784/Desktop/gmail-clone/node_modules:/mnt/c/Users/19784/Desktop/node_modules:/mnt/c/Users/19784/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/19784/Desktop/gmail-clone/node_modules/tailwindcss/lib/node_modules:/mnt/c/Users/19784/Desktop/gmail-clone/node_modules/tailwindcss/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tailwindcss/lib/cli.js" "$@"
else
  exec node  "$basedir/../tailwindcss/lib/cli.js" "$@"
fi
