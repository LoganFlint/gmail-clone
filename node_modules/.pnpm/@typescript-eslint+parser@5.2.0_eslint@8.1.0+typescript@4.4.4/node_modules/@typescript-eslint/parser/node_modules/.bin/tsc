#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/19784/Desktop/gmail-clone/node_modules/.pnpm/typescript@4.4.4/node_modules/typescript/bin/node_modules:/mnt/c/Users/19784/Desktop/gmail-clone/node_modules/.pnpm/typescript@4.4.4/node_modules/typescript/node_modules:/mnt/c/Users/19784/Desktop/gmail-clone/node_modules/.pnpm/typescript@4.4.4/node_modules:/mnt/c/Users/19784/Desktop/gmail-clone/node_modules/.pnpm/node_modules:/mnt/c/Users/19784/Desktop/gmail-clone/node_modules:/mnt/c/Users/19784/Desktop/node_modules:/mnt/c/Users/19784/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/19784/Desktop/gmail-clone/node_modules/.pnpm/@typescript-eslint+parser@5.2.0_eslint@8.1.0+typescript@4.4.4/node_modules/@typescript-eslint/parser/node_modules:/mnt/c/Users/19784/Desktop/gmail-clone/node_modules/.pnpm/@typescript-eslint+parser@5.2.0_eslint@8.1.0+typescript@4.4.4/node_modules/@typescript-eslint/node_modules:/mnt/c/Users/19784/Desktop/gmail-clone/node_modules/.pnpm/@typescript-eslint+parser@5.2.0_eslint@8.1.0+typescript@4.4.4/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/19784/Desktop/gmail-clone/node_modules/.pnpm/typescript@4.4.4/node_modules/typescript/bin/node_modules:/mnt/c/Users/19784/Desktop/gmail-clone/node_modules/.pnpm/typescript@4.4.4/node_modules/typescript/node_modules:/mnt/c/Users/19784/Desktop/gmail-clone/node_modules/.pnpm/typescript@4.4.4/node_modules:/mnt/c/Users/19784/Desktop/gmail-clone/node_modules/.pnpm/node_modules:/mnt/c/Users/19784/Desktop/gmail-clone/node_modules:/mnt/c/Users/19784/Desktop/node_modules:/mnt/c/Users/19784/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/19784/Desktop/gmail-clone/node_modules/.pnpm/@typescript-eslint+parser@5.2.0_eslint@8.1.0+typescript@4.4.4/node_modules/@typescript-eslint/parser/node_modules:/mnt/c/Users/19784/Desktop/gmail-clone/node_modules/.pnpm/@typescript-eslint+parser@5.2.0_eslint@8.1.0+typescript@4.4.4/node_modules/@typescript-eslint/node_modules:/mnt/c/Users/19784/Desktop/gmail-clone/node_modules/.pnpm/@typescript-eslint+parser@5.2.0_eslint@8.1.0+typescript@4.4.4/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../typescript@4.4.4/node_modules/typescript/bin/tsc" "$@"
else
  exec node  "$basedir/../../../../../../typescript@4.4.4/node_modules/typescript/bin/tsc" "$@"
fi
