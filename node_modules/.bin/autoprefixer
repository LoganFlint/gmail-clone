#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/loganflint/Desktop/gmail-clone/node_modules/.pnpm/autoprefixer@10.3.7_postcss@8.3.11/node_modules/autoprefixer/bin/node_modules:/Users/loganflint/Desktop/gmail-clone/node_modules/.pnpm/autoprefixer@10.3.7_postcss@8.3.11/node_modules/autoprefixer/node_modules:/Users/loganflint/Desktop/gmail-clone/node_modules/.pnpm/autoprefixer@10.3.7_postcss@8.3.11/node_modules:/Users/loganflint/Desktop/gmail-clone/node_modules/.pnpm/node_modules:/Users/loganflint/Desktop/gmail-clone/node_modules:/Users/loganflint/Desktop/node_modules:/Users/loganflint/node_modules:/Users/node_modules:/node_modules:/Users/loganflint/Desktop/gmail-clone/node_modules/autoprefixer/bin/node_modules:/Users/loganflint/Desktop/gmail-clone/node_modules/autoprefixer/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/loganflint/Desktop/gmail-clone/node_modules/.pnpm/autoprefixer@10.3.7_postcss@8.3.11/node_modules/autoprefixer/bin/node_modules:/Users/loganflint/Desktop/gmail-clone/node_modules/.pnpm/autoprefixer@10.3.7_postcss@8.3.11/node_modules/autoprefixer/node_modules:/Users/loganflint/Desktop/gmail-clone/node_modules/.pnpm/autoprefixer@10.3.7_postcss@8.3.11/node_modules:/Users/loganflint/Desktop/gmail-clone/node_modules/.pnpm/node_modules:/Users/loganflint/Desktop/gmail-clone/node_modules:/Users/loganflint/Desktop/node_modules:/Users/loganflint/node_modules:/Users/node_modules:/node_modules:/Users/loganflint/Desktop/gmail-clone/node_modules/autoprefixer/bin/node_modules:/Users/loganflint/Desktop/gmail-clone/node_modules/autoprefixer/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../autoprefixer/bin/autoprefixer" "$@"
else
  exec node  "$basedir/../autoprefixer/bin/autoprefixer" "$@"
fi
