#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/loganflint/Desktop/gmail-clone/node_modules/.pnpm/rimraf@3.0.2/node_modules/rimraf/node_modules:/Users/loganflint/Desktop/gmail-clone/node_modules/.pnpm/rimraf@3.0.2/node_modules:/Users/loganflint/Desktop/gmail-clone/node_modules/.pnpm/node_modules:/Users/loganflint/Desktop/gmail-clone/node_modules:/Users/loganflint/Desktop/node_modules:/Users/loganflint/node_modules:/Users/node_modules:/node_modules:/Users/loganflint/Desktop/gmail-clone/node_modules/.pnpm/tmp@0.2.1/node_modules/tmp/node_modules:/Users/loganflint/Desktop/gmail-clone/node_modules/.pnpm/tmp@0.2.1/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/loganflint/Desktop/gmail-clone/node_modules/.pnpm/rimraf@3.0.2/node_modules/rimraf/node_modules:/Users/loganflint/Desktop/gmail-clone/node_modules/.pnpm/rimraf@3.0.2/node_modules:/Users/loganflint/Desktop/gmail-clone/node_modules/.pnpm/node_modules:/Users/loganflint/Desktop/gmail-clone/node_modules:/Users/loganflint/Desktop/node_modules:/Users/loganflint/node_modules:/Users/node_modules:/node_modules:/Users/loganflint/Desktop/gmail-clone/node_modules/.pnpm/tmp@0.2.1/node_modules/tmp/node_modules:/Users/loganflint/Desktop/gmail-clone/node_modules/.pnpm/tmp@0.2.1/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../rimraf@3.0.2/node_modules/rimraf/bin.js" "$@"
else
  exec node  "$basedir/../../../../../rimraf@3.0.2/node_modules/rimraf/bin.js" "$@"
fi
