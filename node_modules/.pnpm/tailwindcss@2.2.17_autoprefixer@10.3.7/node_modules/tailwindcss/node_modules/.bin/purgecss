#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/loganflint/Desktop/gmail-clone/node_modules/.pnpm/purgecss@4.0.3/node_modules/purgecss/bin/node_modules:/Users/loganflint/Desktop/gmail-clone/node_modules/.pnpm/purgecss@4.0.3/node_modules/purgecss/node_modules:/Users/loganflint/Desktop/gmail-clone/node_modules/.pnpm/purgecss@4.0.3/node_modules:/Users/loganflint/Desktop/gmail-clone/node_modules/.pnpm/node_modules:/Users/loganflint/Desktop/gmail-clone/node_modules:/Users/loganflint/Desktop/node_modules:/Users/loganflint/node_modules:/Users/node_modules:/node_modules:/Users/loganflint/Desktop/gmail-clone/node_modules/.pnpm/tailwindcss@2.2.17_autoprefixer@10.3.7/node_modules/tailwindcss/node_modules:/Users/loganflint/Desktop/gmail-clone/node_modules/.pnpm/tailwindcss@2.2.17_autoprefixer@10.3.7/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/loganflint/Desktop/gmail-clone/node_modules/.pnpm/purgecss@4.0.3/node_modules/purgecss/bin/node_modules:/Users/loganflint/Desktop/gmail-clone/node_modules/.pnpm/purgecss@4.0.3/node_modules/purgecss/node_modules:/Users/loganflint/Desktop/gmail-clone/node_modules/.pnpm/purgecss@4.0.3/node_modules:/Users/loganflint/Desktop/gmail-clone/node_modules/.pnpm/node_modules:/Users/loganflint/Desktop/gmail-clone/node_modules:/Users/loganflint/Desktop/node_modules:/Users/loganflint/node_modules:/Users/node_modules:/node_modules:/Users/loganflint/Desktop/gmail-clone/node_modules/.pnpm/tailwindcss@2.2.17_autoprefixer@10.3.7/node_modules/tailwindcss/node_modules:/Users/loganflint/Desktop/gmail-clone/node_modules/.pnpm/tailwindcss@2.2.17_autoprefixer@10.3.7/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../purgecss@4.0.3/node_modules/purgecss/bin/purgecss.js" "$@"
else
  exec node  "$basedir/../../../../../purgecss@4.0.3/node_modules/purgecss/bin/purgecss.js" "$@"
fi
