#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/loganflint/Desktop/gmail-clone/node_modules/.pnpm/vscode-languageserver@8.0.0-next.3/node_modules/vscode-languageserver/bin/node_modules:/Users/loganflint/Desktop/gmail-clone/node_modules/.pnpm/vscode-languageserver@8.0.0-next.3/node_modules/vscode-languageserver/node_modules:/Users/loganflint/Desktop/gmail-clone/node_modules/.pnpm/vscode-languageserver@8.0.0-next.3/node_modules:/Users/loganflint/Desktop/gmail-clone/node_modules/.pnpm/node_modules:/Users/loganflint/Desktop/gmail-clone/node_modules:/Users/loganflint/Desktop/node_modules:/Users/loganflint/node_modules:/Users/node_modules:/node_modules:/Users/loganflint/Desktop/gmail-clone/node_modules/.pnpm/vscode-vue-languageservice@0.27.30/node_modules/vscode-vue-languageservice/node_modules:/Users/loganflint/Desktop/gmail-clone/node_modules/.pnpm/vscode-vue-languageservice@0.27.30/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/loganflint/Desktop/gmail-clone/node_modules/.pnpm/vscode-languageserver@8.0.0-next.3/node_modules/vscode-languageserver/bin/node_modules:/Users/loganflint/Desktop/gmail-clone/node_modules/.pnpm/vscode-languageserver@8.0.0-next.3/node_modules/vscode-languageserver/node_modules:/Users/loganflint/Desktop/gmail-clone/node_modules/.pnpm/vscode-languageserver@8.0.0-next.3/node_modules:/Users/loganflint/Desktop/gmail-clone/node_modules/.pnpm/node_modules:/Users/loganflint/Desktop/gmail-clone/node_modules:/Users/loganflint/Desktop/node_modules:/Users/loganflint/node_modules:/Users/node_modules:/node_modules:/Users/loganflint/Desktop/gmail-clone/node_modules/.pnpm/vscode-vue-languageservice@0.27.30/node_modules/vscode-vue-languageservice/node_modules:/Users/loganflint/Desktop/gmail-clone/node_modules/.pnpm/vscode-vue-languageservice@0.27.30/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../vscode-languageserver@8.0.0-next.3/node_modules/vscode-languageserver/bin/installServerIntoExtension" "$@"
else
  exec node  "$basedir/../../../../../vscode-languageserver@8.0.0-next.3/node_modules/vscode-languageserver/bin/installServerIntoExtension" "$@"
fi
