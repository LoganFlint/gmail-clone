#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/loganflint/Desktop/gmail-clone/node_modules/.pnpm/eslint@8.1.0/node_modules/eslint/bin/node_modules:/Users/loganflint/Desktop/gmail-clone/node_modules/.pnpm/eslint@8.1.0/node_modules/eslint/node_modules:/Users/loganflint/Desktop/gmail-clone/node_modules/.pnpm/eslint@8.1.0/node_modules:/Users/loganflint/Desktop/gmail-clone/node_modules/.pnpm/node_modules:/Users/loganflint/Desktop/gmail-clone/node_modules:/Users/loganflint/Desktop/node_modules:/Users/loganflint/node_modules:/Users/node_modules:/node_modules:/Users/loganflint/Desktop/gmail-clone/node_modules/.pnpm/vue-eslint-parser@7.11.0_eslint@8.1.0/node_modules/vue-eslint-parser/node_modules:/Users/loganflint/Desktop/gmail-clone/node_modules/.pnpm/vue-eslint-parser@7.11.0_eslint@8.1.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/loganflint/Desktop/gmail-clone/node_modules/.pnpm/eslint@8.1.0/node_modules/eslint/bin/node_modules:/Users/loganflint/Desktop/gmail-clone/node_modules/.pnpm/eslint@8.1.0/node_modules/eslint/node_modules:/Users/loganflint/Desktop/gmail-clone/node_modules/.pnpm/eslint@8.1.0/node_modules:/Users/loganflint/Desktop/gmail-clone/node_modules/.pnpm/node_modules:/Users/loganflint/Desktop/gmail-clone/node_modules:/Users/loganflint/Desktop/node_modules:/Users/loganflint/node_modules:/Users/node_modules:/node_modules:/Users/loganflint/Desktop/gmail-clone/node_modules/.pnpm/vue-eslint-parser@7.11.0_eslint@8.1.0/node_modules/vue-eslint-parser/node_modules:/Users/loganflint/Desktop/gmail-clone/node_modules/.pnpm/vue-eslint-parser@7.11.0_eslint@8.1.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../eslint@8.1.0/node_modules/eslint/bin/eslint.js" "$@"
else
  exec node  "$basedir/../../../../../eslint@8.1.0/node_modules/eslint/bin/eslint.js" "$@"
fi
