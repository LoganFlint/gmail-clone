#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/smith/Desktop/Projects/Github/LaunchMail2/launch-mail/node_modules/.pnpm/vscode-languageserver@8.0.0-next.3/node_modules/vscode-languageserver/bin/node_modules:/Users/smith/Desktop/Projects/Github/LaunchMail2/launch-mail/node_modules/.pnpm/vscode-languageserver@8.0.0-next.3/node_modules/vscode-languageserver/node_modules:/Users/smith/Desktop/Projects/Github/LaunchMail2/launch-mail/node_modules/.pnpm/vscode-languageserver@8.0.0-next.3/node_modules:/Users/smith/Desktop/Projects/Github/LaunchMail2/launch-mail/node_modules/.pnpm/node_modules:/Users/smith/Desktop/Projects/Github/LaunchMail2/launch-mail/node_modules:/Users/smith/Desktop/Projects/Github/LaunchMail2/node_modules:/Users/smith/Desktop/Projects/Github/node_modules:/Users/smith/Desktop/Projects/node_modules:/Users/smith/Desktop/node_modules:/Users/smith/node_modules:/Users/node_modules:/node_modules:/Users/smith/Desktop/Projects/Github/LaunchMail2/launch-mail/node_modules/.pnpm/@volar+transforms@0.27.24/node_modules/@volar/transforms/node_modules:/Users/smith/Desktop/Projects/Github/LaunchMail2/launch-mail/node_modules/.pnpm/@volar+transforms@0.27.24/node_modules/@volar/node_modules:/Users/smith/Desktop/Projects/Github/LaunchMail2/launch-mail/node_modules/.pnpm/@volar+transforms@0.27.24/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/smith/Desktop/Projects/Github/LaunchMail2/launch-mail/node_modules/.pnpm/vscode-languageserver@8.0.0-next.3/node_modules/vscode-languageserver/bin/node_modules:/Users/smith/Desktop/Projects/Github/LaunchMail2/launch-mail/node_modules/.pnpm/vscode-languageserver@8.0.0-next.3/node_modules/vscode-languageserver/node_modules:/Users/smith/Desktop/Projects/Github/LaunchMail2/launch-mail/node_modules/.pnpm/vscode-languageserver@8.0.0-next.3/node_modules:/Users/smith/Desktop/Projects/Github/LaunchMail2/launch-mail/node_modules/.pnpm/node_modules:/Users/smith/Desktop/Projects/Github/LaunchMail2/launch-mail/node_modules:/Users/smith/Desktop/Projects/Github/LaunchMail2/node_modules:/Users/smith/Desktop/Projects/Github/node_modules:/Users/smith/Desktop/Projects/node_modules:/Users/smith/Desktop/node_modules:/Users/smith/node_modules:/Users/node_modules:/node_modules:/Users/smith/Desktop/Projects/Github/LaunchMail2/launch-mail/node_modules/.pnpm/@volar+transforms@0.27.24/node_modules/@volar/transforms/node_modules:/Users/smith/Desktop/Projects/Github/LaunchMail2/launch-mail/node_modules/.pnpm/@volar+transforms@0.27.24/node_modules/@volar/node_modules:/Users/smith/Desktop/Projects/Github/LaunchMail2/launch-mail/node_modules/.pnpm/@volar+transforms@0.27.24/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../vscode-languageserver@8.0.0-next.3/node_modules/vscode-languageserver/bin/installServerIntoExtension" "$@"
else
  exec node  "$basedir/../../../../../../vscode-languageserver@8.0.0-next.3/node_modules/vscode-languageserver/bin/installServerIntoExtension" "$@"
fi
